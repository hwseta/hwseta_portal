<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<!-- Actual Training - D1 Wizard Starts-->
		<record id="form_view_actual_training_d1" model="ir.ui.view">
            <field name="name">form.view.actual.training.d1</field>
            <field name="model">actual.training.d1</field>
            <field name="arch" type="xml">
            	<form string="Actual Training - D1">
            		<header>
            			<button name="save_btn" string="Save" type="object" class="oe_highlight"/>
            		</header>
           			<group>
           				<field name="actual_training_fields_ids" nolabel="1">
           					<tree string="Actual Training D1" editable="bottom">
           						<field name="name"/>
           						<field name="surname"/>
           						<field name="code" on_change="onchange_code(code)"/>
           						<field name="major"/>
           						<field name="sub_major_group"/>
           						<field name="occupation"/>
           						<field name="specialization" domain="[('ofo_code_id','=',code)]"/>
           						<field name="municipality"/>
           						<field name="urban"/>
           						<field name="type_training"/>
           						<field name="name_training"/>
           						<field name="training_cost"/>
           						<field name="non_aligned"/>
           						<field name="nqf_level"/>
           						<field name="gender"/>
           						<!--<field name="african"/>
           						<field name="coloured"/>
           						<field name="indian"/>
           						<field name="white"/>-->
           						<field name="age_group"/>
           						<field name="total_cost"/>
           					</tree>
           				</field>
           			</group>
            	</form>
            </field>
        </record>
        <!-- Actual Training - D1 Wizard Ends-->
        
