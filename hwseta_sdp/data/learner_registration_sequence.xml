<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for learner.registration -->
        <record id="seq_type_learner_registration" model="ir.sequence.type">
            <field name="name">learner_registration</field>
            <field name="code">learner.registration.sequence</field>
        </record>

        <record id="seq_learner_registration" model="ir.sequence">
            <field name="name">learner_registration</field>
            <field name="code">learner.registration.sequence</field>
            <field name="prefix">LE</field>
            <field name="padding">8</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        

        <!-- Sequence for SDF Reference Number -->

        <record id="seq_type_sdf_reference_no" model="ir.sequence.type">
            <field name="name">Sdf Register Reference Number</field>
            <field name="code">sdf.register.reference</field>
        </record>

        <record id="seq_sdf_reference_no" model="ir.sequence">
            <field name="name">Sdf Register Reference Number</field>
            <field name="code">sdf.register.reference</field>
            <field name="prefix">R</field>
            <field name="padding">3</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
        <!-- Sequence for WSP -->
        <record id="seq_type_wsp_plan_no" model="ir.sequence.type">
            <field name="name">WSP Plan No</field>
            <field name="code">wsp.plan.reference</field>
        </record>
        
        <record id="seq_sdf_wsp_plan_no" model="ir.sequence">
            <field name="name">WSP Plan Number</field>
            <field name="code">wsp.plan.reference</field>
            <field name="prefix">WSP</field>
            <field name="padding">3</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
        <!-- Sequence for Monitoring and Evaluation -->
        <record id="seq_type_monitor_and_evaluate" model="ir.sequence.type">
            <field name="name">Monitoring and Evaluation</field>
            <field name="code">monitor.and.evaluate.ref</field>
        </record>
        
        <record id="seq_monitoring_and_evaluate" model="ir.sequence">
            <field name="name">Monitoring and Evaluation</field>
            <field name="code">monitor.and.evaluate.ref</field>
            <field name="prefix">ME</field>
            <field name="padding">3</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
		<!-- Sequences for EOI ID master.  -->
        <record id="seq_type_eoi_id" model="ir.sequence.type">
            <field name="name">EOI Id</field>
            <field name="code">eoi_id.configuration</field>
        </record>
        <record id="seq_eoi_id" model="ir.sequence">
            <field name="name">EOI Id</field>
            <field name="code">eoi_id.configuration</field>
            <field name="prefix">EOI-ID</field>
            <field name="padding">7</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
    </data>
</openerp>
