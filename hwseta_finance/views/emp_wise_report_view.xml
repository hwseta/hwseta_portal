<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="report_levy_report123">
    <t t-call="report.external_layout">
        <div class="page">
            <div class="oe_structure"/>
            <div class="row">
                
            </div>

            <center><h3>
                Account Ledger Report
            </h3></center>
			
			<table class="table table-condensed" style="border: 1px solid black">
                
                <tr style="border: 1px solid black">
                        <th style="border: 1px solid black">Account</th>
                        <th style="border: 1px solid black">Analytic Account</th>
                        <th style="border: 1px solid black" >Entry Number</th>
                        <th style="border: 1px solid black">Journal</th>
                        <th style="border: 1px solid black">Period</th>
                        <th style="border: 1px solid black">Effective Date</th>
                        <th style="border: 1px solid black">Name</th>
                        <th style="border: 1px solid black">reference</th>
                        <th style="border: 1px solid black">Partner</th>
                        <th style="border: 1px solid black">tax</th>
                        <th style="border: 1px solid black">Debit</th>
                        <th style="border: 1px solid black">Credit</th>
                        <!-- <th >Reconcile reference</th> -->
                    </tr>
                     <t t-foreach="get_account(data)" t-as="k7">
                    <!-- <tr>
                   <table class="table table-condensed" border="1" style="border: 1px solid black" >  -->  
                    <tr style="border: 1px solid black">
                        <td style="border: 1px solid black"><span t-esc="k7.name"/></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <!-- <td></td> -->
                    </tr>
                    <t t-foreach="get_ana_account(k7.id)" t-as="an">
                    <tr>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"><span t-esc="an.name"/></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <!-- <td></td> -->
                    </tr>
                    <tr t-foreach="get_lines(k7.id,an.id)" t-as="l">
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"></td>
                        <td style="border: 1px solid black"><span t-esc="l.move_id.name or 0.0"/></td> 
                        <td style="border: 1px solid black"><span t-esc="l.journal_id.name or 0.0"/></td>
                        <td style="border: 1px solid black"><span t-esc="l.period_id.name or 0.0" /></td>
                        <td style="border: 1px solid black"><span t-esc="l.date or 0.0"/></td>
                        <td style="border: 1px solid black"><span t-esc="l.name or 0.0"/></td>
                        <td style="border: 1px solid black"><span t-esc="l.ref or 0.0" /></td>
                        <td style="border: 1px solid black"><span t-esc="l.partner_id.name or 0.0" /> </td>
                        <td style="border: 1px solid black"><span t-esc="l.account_tax_id.name or 0.0" /></td>
                        <td style="border: 1px solid black"><span t-esc="l.debit or 0.0" /></td>
                        <td style="border: 1px solid black"><span t-esc="l.credit or 0.0"/></td>
                        <!-- <td><span t-esc="l.reconcile_ref or 0.0"/></td> -->
                    </tr>
                    </t>
                    <!-- </table>
                    </tr> -->
                     </t> 
                
               
            </table>
            <div class="oe_structure"/>
        </div>
    </t>
</template>

